<div class="container">
  <h2>いいね一覧</h2>
  <div>
    <% @like_posts.each do |post|%>
      <p><%= attachment_image_tag post.user, :profile_image, :fill, 60, 60, fallback: "noimage.png", size: '60×60', class: "icon" %>
      <%= post.user.name %></p>
      <p><%= post.body %></p>
    <% end %>
    </div>
</div>